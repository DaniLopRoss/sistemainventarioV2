<head>
<%= stylesheet_link_tag "style" %>
</head>
<div class="container">
<div class="card mt-5">
<div class="card-header">
<ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="">Insumo</a></li>
          <li class="breadcrumb-item active">Características Insumo</li>
        </ol>



<h5 class="text">Características insumo <%= @resource.id %> </h5>
</div>
 <div class="probar">



<%= render @resource %>
<br>
</br>
<div>
<%if can? :edit, @resource%>
  <%= link_to "Editar ", edit_resource_path(@resource),  class: "btn btn-warning btn-xs round btn-editar " %>  
  <%end%> 
  <%if can? :destroy, @resource%>
    <%= link_to "Eliminar ", @resource, method: :delete , data: { confirm: "Esta seguro de eliminar"},  class: "btn btn-danger btn-eliminar" %>
    <%end%>
  <%= link_to "Regresar ", resources_path, class: "btn btn-success btn-xs round btn-regresar " %>
<br>
</br>
</div>
