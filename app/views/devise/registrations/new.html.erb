<body>
 

  <div id="main-container" class="container-fluid bg-gray-100">
    <br>
    </br>
    
      <div
        class="flex flex-col md:flex-row w-full md:w-2/3 h-full md:h-auto bg-white rounded-lg md:shadow px-2 py-12 md:px-8 md:py-8">
        <div class="hidden md:flex md:flex-col justify-center w-1/3 mr-12 pl-6">

          </p>
        </div>
        <h5 class="eti">Registrar Usuario</h5>
        <div class="pt-4 pb-6">

          <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <div>
            <%= render "devise/shared/error_messages", resource: resource %>
          </div>

          <div class="form-group row">
            <%= f.label :nombre, "Nombre:", class:'col-sm-2'  %><br />
            <div class="col-sm-8">
              <%= f.text_field :nombre, class:"form-control" %>
            </div>
          </div>

          <div class="form-group row">
            <%= f.label :id, "RFC:", class:'col-sm-2'  %><br />
            <div class="col-sm-8">
              <input type="text" onkeyup="mayus(this);" <%= f.text_field :id, class:"form-control" %> </div> </div> <div
                class="form-group row">
              <%= f.label :apellidouno, "Apellido Paterno:", class:'col-sm-2'  %><br />
              <div class="col-sm-8">
                <%= f.text_field :apellidouno, class:"form-control" %>
              </div>
            </div>
            <div class="form-group row">
              <%= f.label :apellidodos, "Apellido Materno:", class:'col-sm-2'  %><br />
              <div class="col-sm-8">
                <%= f.text_field :apellidodos, class:"form-control" %>
              </div>
            </div>
            <div class="form-group row">
              <%= f.label :role, "Rol:", class:'col-sm-2'  %><br />
              <div class="col-sm-8">
                <%= f.select :role, ["Administrador", "Agente", "Personal"], prompt: "Seleccione el rol" %>
              </div>
            </div>

            <div class="form-group row">
              <%= f.label :email, "Correo:", class:'col-sm-2'  %><br />
              <div class="col-sm-8">
                <%= f.email_field :email, autofocus: true, autocomplete: "email", class:"form-control"  %>
              </div>
            </div>

            <div class="form-group row">
              <%= f.label :password_plain, "Contraseña:", class: 'col-sm-2' %>
              <div class="col-sm-8">
                <%= f.password_field :password_plain, autocomplete: "new-password", class: "form-control" %>
              </div>
            </div>
            <div class="form-group row">
              <%= f.label :password, "Contraseña:", class:'col-sm-2'  %>
              <div class="col-sm-8">
                <%= f.password_field :password, autocomplete: "new-password", class:"form-control"  %>
              </div>
            </div>
            <div class="form-group row">
              <%= f.label :password_confirmation, "Confirma Contraseña:", class:'col-sm-2' %><br />
              <div class="col-sm-8">
                <%= f.password_field :password_confirmation, autocomplete: "new-password", class:"form-control"  %>
              </div>
            </div>
            <br>
            <div class="actions">
              <%= f.submit "Registrarse ", class: "btn btn-primary" %>
              <%= link_to 'Cancelar', url_for(:back), class: "btn btn-warning" %>
              <br>
            </div>
            <br>
            <% end %>

          </div>
        </div>
      </div>

    </div>
  </div>

